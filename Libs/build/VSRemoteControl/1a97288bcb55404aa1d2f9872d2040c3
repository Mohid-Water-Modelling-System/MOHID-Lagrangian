          </unzip>
          <setRegKey>
            <keyPath>HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\VisualStudio\14.0\Setup\VS\SecondaryInstaller\Ant</keyPath>
            <valueName>ANT_HOME</valueName>
            <valueValue>%SI_VSFolder%Apps\apache-ant-1.9.3\</valueValue>
          </setRegKey>
          <setRegKey>
            <keyPath>HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\VisualStudio\14.0\Setup\VS\SecondaryInstaller\Ant</keyPath>
            <valueName>Install</valueName>
            <valueValue>1</valueValue>
          </setRegKey>
          <setRegKey>
            <keyPath>HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\VisualStudio\14.0\Setup\VS\SecondaryInstaller\Ant</keyPath>
            <valueName>Version</valueName>
            <valueValue>1.9.3</valueValue>
          </setRegKey>
        </installCommands>
        <!-- Repair Command example. -->
        <repairCommands>
          <!-- Get the path of VS Folder to %SI_VSFolder% at Process level. -->
          <getRegKey>
            <keyPath>HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\VisualStudio\14.0</keyPath>
            <valueName>ShellFolder</valueName>
            <envVar>SI_VSFolder</envVar>
          </getRegKey>
          <unzip>
            <!-- InstallerFile is the zip file downloaded -->
            <zipFile>%InstallerFile%</zipFile>
            <targetDirectory>%SI_VSFolder%Apps\</targetDirectory>
          </unzip>
          <setRegKey>
            <keyPath>HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\VisualStudio\14.0\Setup\VS\SecondaryInstaller\Ant</keyPath>
            <valueName>ANT_HOME</valueName>
            <valueValue>%SI_VSFolder%Apps\apache-ant-1.9.3\</valueValue>
          </setRegKey>
          <setRegKey>
            <keyPath>HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\VisualStudio\14.0\Setup\VS\SecondaryInstaller\Ant</keyPath>
            <valueName>Install</valueName>
            <valueValue>1</valueValue>
          </setRegKey>
          <setRegKey>
            <keyPath>HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\VisualStudio\14.0\Setup\VS\SecondaryInstaller\Ant</keyPath>
            <valueName>Version</valueName>
            <valueValue>1.9.3</valueValue>
          </setRegKey>
        </repairCommands>
        <!-- Uninstall Command example. -->
        <uninstallCommands>
          <!-- Get the path of VS Folder to %SI_VSFolder% at Process level. -->
          <getRegKey>
            <keyPath>HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\VisualStudio\14.0</keyPath>
            <valueName>ShellFolder</valueName>
            <envVar>SI_VSFolder</envVar>
          </getRegKey>
          <remove>
            <items>%SI_VSFolder%Apps\apache-ant-1.9.3</items>
          </remove>
          <removeReg>
            <keyPath>HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\VisualStudio\14.0\Setup\VS\SecondaryInstaller\Ant</keyPath>
          </removeReg>
          <removeReg>
            <keyPath>HKEY_LOCAL_MACHINE\SOFTWARE\WOW6432Node\Microsoft\VisualStudio\14.0\Setup\VS\SecondaryInstaller\Ant</keyPath>
          </removeReg>
        </uninstallCommands>
      </installer>
    </installers>
  </entry>
