          <sha256>5F59451794BC5F446D05834783269549367D1F2A6FF1C14A8669597D07284734</sha256>
        </installerFile>
        <logFiles>
          <location>%temp%\secondaryinstaller_websocket4net.log</location>
        </logFiles>
