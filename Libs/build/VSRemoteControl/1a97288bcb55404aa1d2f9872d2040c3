          </unzip>
          <copy>
            <source>%LOCALAPPDATA%\Microsoft\VisualStudioSecondaryInstaller\14.0\installers\WebSocket4Net\lib\net45\WebSocket4Net.dll</source>
            <target>%SI_VSFolder%Common7\IDE\CommonExtensions\Microsoft\WebClient\Diagnostics\ToolWindows\WebSocket4Net.dll</target>
            <overwriteTarget>false</overwriteTarget>
          </copy>
        </installCommands>
